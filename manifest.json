{
  "manifest_version":2,
  "name": " My Cool Extension",
  "version": "1.2",
  "content_scripts": [
    {
      "matches":[
        /* "http://github.com/*",
        "https://github.com/*" */
        "<all_urls>"
        /* "*://*github*" */
      ],
      "js": ["content.js"]
    }
  ],
  "background":{
    "scripts": ["background.js"]
  },
  "browser_action":{
    "default_icon":"m.png"
  }
}
